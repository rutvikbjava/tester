{
  "name": "magic-backend",
  "version": "1.0.0",
  "description": "Backend API for MAGIC Startup Incubation Management System",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "prisma generate",
    "postinstall": "prisma generate",
    "seed": "node scripts/seed.js",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "db:setup": "npx prisma migrate dev --name init && node prisma/seed.js",
    "db:test": "node test-database.js",
    "db:migrate-data": "node migrate-localstorage-to-db.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [
    "incubation",
    "startup",
    "management"
  ],
  "author": "CMIA Marathwada",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@supabase/supabase-js": "^2.87.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "5.22.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
